{
	"Joomla Blank Form Field": {
		"prefix": "sp-form",
		"body": [
			"<?xml version=\"1.0\" encoding=\"utf-8\"?>",
			"<form>",
			"\t<fields name=\"${1:fieldsName}\" label=\"${2:fieldsLabel}\">",
			"\t\t<field type=\"${3:fieldType}\" name=\"${4:fieldName}\" label=\"${5:fieldLabel}\"></field>",
			"\t</fields>",
			"</form>"
		],
		"description": "Joomla Blank Form Field"
	},
	"Joomla Blank Config Field": {
		"prefix": "sp-config",
		"body": [
			"<?xml version=\"1.0\" encoding=\"utf-8\"?>",
			"<config>",
			"\t<inlinehelp button=\"show\"/>",
			"\t<fieldset name=\"${1:example}\" label=\"${2:example}\" description=\"${3:example}\">",
			"\t\t<field type=\"${4:example}\" name=\"${5:example}\" label=\"${6:example}\"></field>",
			"\t</fieldset>",
			"</config>"
		],
		"description": "Joomla Blank Config Field"
	},
	"Joomla Blank Config Without Field": {
		"prefix": "sp-wconfig",
		"body": [
			"<?xml version=\"1.0\" encoding=\"utf-8\"?>",
			"<config>",
			"<inlinehelp button=\"show\"/>",
			"\t<fieldset name=\"${1:example}\" label=\"${2:example}\" description=\"${3:example}\">",
			"\t\t${4:example}",
			"\t</fieldset>",
			"</config>"
		],
		"description": "Joomla Blank Config Without Field"
	},
	"Joomla Blank Form Fieldset": {
		"prefix": "sp-fieldset",
		"body": [
			"\t<fieldset name=\"${1:example}\" label=\"${2:example}\" description=\"${3:example}\">",
			"\t${4:example}",
			"\t</fieldset>"
		],
		"description": "Joomla Blank Form Fieldset"
	},
	"Joomla Blank Form Fields": {
		"prefix": "sp-fields",
		"body": [
			"<fields name=\"${1:example}\" label=\"${2:example}\">",
			"\t${3:example}",
			"</fields>"
		],
		"description": "Joomla Blank Form Fields"
	},
	"Joomla Blank Form Text field": {
		"prefix": "sp-textfield",
		"body": [
			"<field",
			"\ttype=\"text\"",
			"\tname=\"${1:example}\"",
			"\tlabel=\"${2:example}\"",
			"/>"
		],
		"description": "Joomla Blank Form Text Field"
	},
	"Joomla Blank Form Number field": {
		"prefix": "sp-numberfield",
		"body": [
			"<field",
			"\ttype=\"number\"",
			"\tname=\"${1:example}\" ",
			"\tlabel=\"${2:example}\"",
			"/>"
		],
		"description": "Joomla Blank Form Number Field"
	},
	"Joomla Blank Form Radio Field": {
		"prefix": "sp-radiofield",
		"body": [
			"<field",
			"name=\"${1:example}\"",
			"type=\"radio\"",
			"layout=\"joomla.form.field.radio.switcher\"",
			"label=\"${2:example}\" default=\"1\"",
			">",
			"\t<option value=\"0\">JHIDE</option>",
			"\t<option value=\"1\">JSHOW</option>",
			"</field>"
		],
		"description": "Joomla Blank Form Radio Field"
	},
	"Joomla Blank Form Custom Field": {
		"prefix": "sp-customfield",
		"body": [
			"<field",
			"\tname=\"${1:name}\"",
			"\ttype=\"${2:type}\"",
			"\tlabel=\"${3:label}\"",
			"\taddfieldprefix=\"${4:namespacePath}\"/>"
		],
		"description": "Joomla Blank Form Custom Field"
	},
	"Joomla Blank Form Media Field": {
		"prefix": "sp-mediafield",
		"body": [
			"<field",
			"\ttype=\"media\"",
			"\tname=\"${1:example}\" ",
			"\tlabel=\"${1:example}\"",
			"/>",
			""
		],
		"description": "Joomla Blank Form Media Field"
	},
	"Joomla Blank Form Checkbox Field": {
		"prefix": "sp-checkboxfield",
		"body": [
			"<field",
			"\ttype=\"checkbox\"",
			"\tname=\"${1:example}\" ",
			"\tlabel=\"${2:example}\"",
			"\tdescription=\"${3:example}\"",
			"\tvalue=\"${4:example}\"",
			"\tchecked=\"${5:example}\"",
			"/>",
			""
		],
		"description": "Joomla Blank Form Checkbox Field"
	},
	"Joomla Blank Form Color Field": {
		"prefix": "sp-colorfield",
		"body": [
			"<field",
			"\ttype=\"color\"",
			"\tname=\"${1:example}\" ",
			"\tlabel=\"${2:example}\"",
			"/>",
			""
		],
		"description": "Joomla Blank Form Color Field"
	},
	"Joomla Blank Form Editor Field": {
		"prefix": "sp-editorfield",
		"body": [
			"<field",
			"\ttype=\"editor\"",
			"\tname=\"${1:example}\" ",
			"\tlabel=\"${2:example}\"",
			"/>",
			""
		],
		"description": "Joomla Blank Form Editor Field"
	},
	"Joomla Blank Form File Field": {
		"prefix": "sp-filefield",
		"body": [
			"<field",
			"\ttype=\"file\"",
			"\tname=\"${1:example}\" ",
			"\tlabel=\"${2:example}\"",
			"/>",
			""
		],
		"description": "Joomla Blank Form File Field"
	},
	"Joomla Blank Form Integer Field": {
		"prefix": "sp-integerfield",
		"body": [
			"<field",
			"\ttype=\"integer\"",
			"\tname=\"${1:example}\" ",
			"\tlabel=\"${2:example}\"",
			"/>",
			""
		],
		"description": "Joomla Blank Form Integer Field"
	},
	"Joomla Blank Subform Field With Form Source": {
		"prefix": "sp-subform-formsource",
		"body": [
			"<field",
			"\tname=\"${1:example}\"",
			"\ttype=\"subform\"",
			"\tformsource=\"${2:example}\"",
			"\tlabel=\"${3:example}\" ",
			"\tdescription=\"${4:example}\"",
			"/>"
		],
		"description": "Joomla Blank Subform Field With Form Source"
	},
	"Joomla Blank Subform Field": {
		"prefix": "sp-subform",
		"body": [
			"<field",
			"name=\"${1:name}\"",
			"type=\"subform\"",
			"label=\"${2:label}\"",
			"description=\"${3:description}\"",
			"multiple=\"true\"",
			">",
			"<form>",
			"${4:sp-}",
			"</form>",
			"</field>"
		],
		"description": "Joomla Blank Subform Field"
	},
	"Joomla Menu Item": {
		"prefix": "sp-menu",
		"body": [
			"<?xml version=\"1.0\" encoding=\"utf-8\"?>",
			"<metadata>",
				"\t<layout title=\"${1:Menu Title}\">",
					"\t\t<message><![CDATA[${2:Menu Description}]]></message>",
				"\t</layout>",
			"</metadata>"
		],
		"description": "Joomla Blank Menu Item"
	},
	"Joomla Blank Form List Field": {
		"prefix": "sp-listfield",
		"body": [
			"<field",
			"\ttype=\"list\"",
			"\tname=\"${1:example}\"",
			"\tlabel=\"${2:example}\"",
			">",
			"\t<option value=\"option1\">Option 1</option>",
			"\t<option value=\"option2\">Option 2</option>",
			"\t<option value=\"option3\">Option 3</option>",
			"</field>"
		],
		"description": "Joomla Blank Form List Field"
	},
	"Joomla Blank Form Textarea Field": {
		"prefix": "sp-textareafield",
		"body": [
			"<field",
			"\ttype=\"textarea\"",
			"\tname=\"${1:example}\"",
			"\tlabel=\"${2:example}\"",
			"\trows=\"5\"",
			"\tcols=\"30\"",
			"\tfilter=\"raw\"",
			"/>"
		],
		"description": "Joomla Blank Form Textarea Field"
	},
	"Joomla Blank Form SQL Field": {
		"prefix": "sp-sqlfield",
		"body": [
			"<field",
			"\ttype=\"sql\"",
			"\tname=\"${1:example}\"",
			"\tlabel=\"${2:example}\"",
			"\tquery=\"SELECT id, title FROM #__table\"",
			"\tkey_field=\"id\"",
			"\tvalue_field=\"title\"",
			"/>"
		],
		"description": "Joomla Blank Form SQL Field"
	},
	"Joomla Blank Form Calendar Field": {
		"prefix": "sp-calendarfield",
		"body": [
			"<field",
			"\ttype=\"calendar\"",
			"\tname=\"${1:example}\"",
			"\tlabel=\"${2:example}\"",
			"\tformat=\"%Y-%m-%d\"",
			"\tsize=\"10\"",
			"\tmaxlength=\"10\"",
			"/>"
		],
		"description": "Joomla Blank Form Calendar Field"
	},
	"Joomla Blank Form Hidden Field": {
		"prefix": "sp-hiddenfield",
		"body": [
			"<field",
			"\ttype=\"hidden\"",
			"\tname=\"${1:example}\"",
			"\tdefault=\"${2:defaultValue}\"",
			"/>"
		],
		"description": "Joomla Blank Form Hidden Field"
	},
	"Joomla Blank Form Spacer Field": {
		"prefix": "sp-spacerfield",
		"body": [
			"<field",
			"\ttype=\"spacer\"",
			"\tname=\"${1:example}\"",
			"\thr=\"true\"",
			"\tlabel=\"${2:example}\"",
			"/>"
		],
		"description": "Joomla Blank Form Spacer Field"
	},
	"Joomla Blank Form Repeatable Field": {
		"prefix": "sp-repeatablefield",
		"body": [
			"<field",
			"\ttype=\"repeatable\"",
			"\tname=\"${1:example}\"",
			"\tlabel=\"${2:example}\"",
			"\tmax=\"10\"",
			">",
			"\t<form>",
			"\t\t<field",
			"\t\t\ttype=\"text\"",
			"\t\t\tname=\"item\"",
			"\t\t\tlabel=\"Item\"",
			"\t\t/>",
			"\t</form>",
			"</field>"
		],
		"description": "Joomla Blank Form Repeatable Field"
	}
}